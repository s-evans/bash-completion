                                    randomizeidentifiers\=enabled store\=active
                                    store\=persistent
                                    addressmaskreply\=disabled
                                    addressmaskreply\=enabled minmtu\=' 
                                    ;;

                                interface)
                                    if [[ $count -eq 4 ]]; then
                                        local IFS=$'\n'
                                        options=$(_netsh_interfaces)
                                    else
                                        options='forwarding\=disabled
                                        forwarding\=enabled advertise\=disabled
                                        advertise\=enabled mtu\= siteid\=
                                        metric\= metric\=automatic
                                        firewall\=disabled firewall\=enabled
