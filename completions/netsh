                                    options=$(_netsh_wlan_interfaces)
                                fi
                                ;;

                        esac

                    fi
                    ;;

                disconnect)
                    if [[ $count -eq 2 ]]; then
                        local IFS=$'\n'
                        options=$(_netsh_wlan_interfaces)
                    fi
                    ;;

                dump)
                    ;;

