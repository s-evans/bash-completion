                                return
                            fi

                            case $subsubsubcmd in
                                address)
                                    if [[ $count -eq 4 ]]; then
                                        local IFS=$'\n'
                                        options=$(_netsh_interfaces)
                                    elif [[ $count -eq 5 ]]; then
                                        options='dhcp static'
                                    elif [[ $count -eq 6 ]]; then
