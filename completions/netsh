                                elif [[ $count -eq 4 ]]; then
                                    options='all current'
                                elif [[ $count -eq 5 ]]; then
                                    local IFS=$'\n'
                                    options=$(_netsh_wlan_interfaces)
                                fi
                                ;;

                            tracing)
                                if [[ $count -eq 3 ]]; then
                                    options='yes no persistent'
                                fi
                                ;;

                        esac

                    fi
                    ;;

                show)
                    if [[ $count -eq 2 ]]; then
                        options="all allowexplicitcreds autoconfig \
                            blockednetworks createalluserprofile drivers filters \
                            hostednetwork interfaces networks  \
                            onlyUseGPProfilesforAllowedNetworks profiles settings \
                            tracing"
                    else

                        case $subsubcmd in
                            filters)
                                if [[ $count -eq 3 ]]; then
                                    options='allow block'
                                fi
                                ;;

                            hostednetwork)
                                if [[ $count -eq 3 ]]; then
                                    options='security'
                                fi
                                ;;

                            networks)
                                if [[ $count -eq 3 ]]; then
                                    local IFS=$'\n'
                                    options=$(_netsh_wlan_interfaces)
                                elif [[ $count -eq 4 ]]; then
                                    options='ssid bssid'
                                fi
                                ;;

                            profiles)
                                if [[ $count -eq 3 ]]; then
                                    local IFS=$'\n'
                                    options=$(_netsh_wlan_profiles)
                                elif [[ $count -eq 4 ]]; then
                                    local IFS=$'\n'
                                    options=$(_netsh_wlan_interfaces)
                                elif [[ $count -eq 5 ]]; then
                                    options='key\=clear'
                                fi
                                ;;
                        esac
                    fi
                    ;;

                start)
                    if [[ $count -eq 2 ]]; then
                        options='hostednetwork'
                    fi
                    ;;

                stop)
                    if [[ $count -eq 2 ]]; then
                        options='hostednetwork'
                    fi
                    ;;

            esac

            ;;

        *)
            # TODO: cull the options list
            options="-a -c -r -u -p -f ${commands[@]}" 
            ;;

    esac

    if [[ "$prev" != "?" && -z "$IFS" ]]; then
        options="$options"
    fi

    if [ -n "$options" -a -z "$COMPREPLY" ]; then
        COMPREPLY=( $(compgen -W '$options' -- "$cur" ) )
    fi

    [[ $COMPREPLY == *= ]] && compopt -o nospace

    return 0
}

complete -o default -F _netsh netsh netsh.exe

